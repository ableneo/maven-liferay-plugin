<component-set>
    <components>
        <component>
            <role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
            <role-hint>liferayPortal</role-hint>
            <implementation>
                org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
            </implementation>
            <configuration>
                <lifecycles>
                    <lifecycle>
                        <id>default</id>
                        <phases>
                            <process-resources>
                                org.mimacom.maven.plugins:mimacom-liferay-builder-maven-plugin:updatePortalPom,
                                org.apache.maven.plugins:maven-resources-plugin:resources
                            </process-resources>
                            <compile>
                                org.apache.maven.plugins:maven-compiler-plugin:compile
                            </compile>
                            <process-test-resources>
                                org.apache.maven.plugins:maven-resources-plugin:testResources
                            </process-test-resources>
                            <test-compile>
                                org.apache.maven.plugins:maven-compiler-plugin:testCompile
                            </test-compile>
                            <test>
                                org.apache.maven.plugins:maven-surefire-plugin:test
                            </test>
                            <package>
                                org.mimacom.maven.plugins:mimacom-liferay-builder-maven-plugin:updatePortal
                            </package>
                            <install>
                                org.mimacom.maven.plugins:mimacom-liferay-builder-maven-plugin:createPortal,
                                org.mimacom.maven.plugins:mimacom-liferay-builder-maven-plugin:installWar,
                                org.apache.maven.plugins:maven-install-plugin:install
                            </install>
                            <deploy>
                                org.apache.maven.plugins:maven-deploy-plugin:deploy
                            </deploy>
                        </phases>
                    </lifecycle>
                </lifecycles>
            </configuration>
        </component>
        <component>
            <role>org.apache.maven.artifact.handler.ArtifactHandler</role>
            <role-hint>liferayPortal</role-hint>
            <implementation>
                org.apache.maven.artifact.handler.DefaultArtifactHandler
            </implementation>
            <configuration>
                <type>liferayPortal</type>
                <extension>jar</extension>
                <language>java</language>
                <addedToClasspath>true</addedToClasspath>
            </configuration>
        </component>

        <component>
            <role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
            <role-hint>liferayPortlet</role-hint>
            <implementation>
                org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
            </implementation>
            <configuration>
                <lifecycles>
                    <lifecycle>
                        <id>default</id>
                        <phases>
                            <process-resources>
                                org.apache.maven.plugins:maven-resources-plugin:resources
                            </process-resources>
                            <compile>
                                org.apache.maven.plugins:maven-compiler-plugin:compile
                            </compile>
                            <process-test-resources>
                                org.apache.maven.plugins:maven-resources-plugin:testResources
                            </process-test-resources>
                            <test-compile>
                                org.apache.maven.plugins:maven-compiler-plugin:testCompile
                            </test-compile>
                            <test>
                                org.apache.maven.plugins:maven-surefire-plugin:test
                            </test>
                            <package>
                                org.apache.maven.plugins:maven-war-plugin:war,
                                org.mimacom.maven.plugins:mimacom-liferay-builder-maven-plugin:createWar
                            </package>
                            <install>
                                org.mimacom.maven.plugins:mimacom-liferay-builder-maven-plugin:installWar,
                                org.apache.maven.plugins:maven-install-plugin:install
                            </install>
                            <deploy>org.apache.maven.plugins:maven-deploy-plugin:deploy
                            </deploy>
                        </phases>
                    </lifecycle>
                    <lifecycle>
                        <id>clean</id>
                        <phases>
                            <clean>
                                org.apache.maven.plugins:maven-clean-plugin:clean,
                                org.mimacom.maven.plugins:mimacom-liferay-builder-maven-plugin:cleanPortlet
                            </clean>
                        </phases>
                    </lifecycle>
                </lifecycles>
            </configuration>
        </component>
        <component>
            <role>org.apache.maven.artifact.handler.ArtifactHandler</role>
            <role-hint>liferayPortlet</role-hint>
            <implementation>
                org.apache.maven.artifact.handler.DefaultArtifactHandler
            </implementation>
            <configuration>
                <type>liferayPortlet</type>
                <extension>jar</extension>
                <language>java</language>
                <addedToClasspath>true</addedToClasspath>
            </configuration>
        </component>

        <component>
            <role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
            <role-hint>liferayServiceApi</role-hint>
            <implementation>
                org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
            </implementation>
            <configuration>
                <lifecycles>
                    <lifecycle>
                        <id>default</id>
                        <phases>
                            <generate-sources>
                                org.mimacom.maven.plugins:mimacom-liferay-builder-maven-plugin:buildService
                            </generate-sources>
                            <process-resources>
                                org.apache.maven.plugins:maven-resources-plugin:resources
                            </process-resources>
                            <compile>
                                org.apache.maven.plugins:maven-compiler-plugin:compile
                            </compile>
                            <process-test-resources>
                                org.apache.maven.plugins:maven-resources-plugin:testResources
                            </process-test-resources>
                            <test-compile>
                                org.apache.maven.plugins:maven-compiler-plugin:testCompile
                            </test-compile>
                            <test>
                                org.apache.maven.plugins:maven-surefire-plugin:test
                            </test>
                            <package>
                                org.apache.maven.plugins:maven-jar-plugin:jar
                            </package>
                            <install>
                                org.mimacom.maven.plugins:mimacom-liferay-builder-maven-plugin:installService,
                                org.apache.maven.plugins:maven-install-plugin:install
                            </install>
                            <deploy>
                                org.apache.maven.plugins:maven-deploy-plugin:deploy
                            </deploy>
                        </phases>
                    </lifecycle>
                </lifecycles>
            </configuration>
        </component>
        <component>
            <role>org.apache.maven.artifact.handler.ArtifactHandler</role>
            <role-hint>liferayServiceApi</role-hint>
            <implementation>
                org.apache.maven.artifact.handler.DefaultArtifactHandler
            </implementation>
            <configuration>
                <type>liferayServiceApi</type>
                <extension>jar</extension>
                <language>java</language>
                <addedToClasspath>true</addedToClasspath>
            </configuration>
        </component>

        <component>
            <role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
            <role-hint>liferayServiceImpl</role-hint>
            <implementation>
                org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
            </implementation>
            <configuration>
                <lifecycles>
                    <lifecycle>
                        <id>default</id>
                        <phases>
                            <generate-sources>
                                org.mimacom.maven.plugins:mimacom-liferay-builder-maven-plugin:buildService
                            </generate-sources>
                            <process-resources>
                                org.apache.maven.plugins:maven-resources-plugin:resources
                            </process-resources>
                            <compile>
                                org.apache.maven.plugins:maven-compiler-plugin:compile
                            </compile>
                            <process-test-resources>
                                org.apache.maven.plugins:maven-resources-plugin:testResources
                            </process-test-resources>
                            <test-compile>
                                org.apache.maven.plugins:maven-compiler-plugin:testCompile
                            </test-compile>
                            <test>
                                org.apache.maven.plugins:maven-surefire-plugin:test
                            </test>
                            <package>
                                org.apache.maven.plugins:maven-war-plugin:war
                            </package>
                            <install>
                                org.mimacom.maven.plugins:mimacom-liferay-builder-maven-plugin:installService,
                                org.mimacom.maven.plugins:mimacom-liferay-builder-maven-plugin:installWar,
                                org.apache.maven.plugins:maven-install-plugin:install
                            </install>
                            <deploy>
                                org.apache.maven.plugins:maven-deploy-plugin:deploy
                            </deploy>
                        </phases>
                    </lifecycle>
                </lifecycles>
            </configuration>
        </component>
        <component>
            <role>org.apache.maven.artifact.handler.ArtifactHandler</role>
            <role-hint>liferayServiceImpl</role-hint>
            <implementation>
                org.apache.maven.artifact.handler.DefaultArtifactHandler
            </implementation>
            <configuration>
                <type>liferayServiceImpl</type>
                <extension>jar</extension>
                <language>java</language>
                <addedToClasspath>true</addedToClasspath>
            </configuration>
        </component>

        <component>
            <role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
            <role-hint>liferayService</role-hint>
            <implementation>
                org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
            </implementation>
            <configuration>
                <lifecycles>
                    <lifecycle>
                        <id>default</id>
                        <phases>
                            <generate-sources>
                                org.mimacom.maven.plugins:mimacom-liferay-builder-maven-plugin:buildService2
                            </generate-sources>
                            <process-resources>
                                org.apache.maven.plugins:maven-resources-plugin:resources
                            </process-resources>
                            <compile>
                                org.apache.maven.plugins:maven-compiler-plugin:compile
                            </compile>
                            <process-test-resources>
                                org.apache.maven.plugins:maven-resources-plugin:testResources
                            </process-test-resources>
                            <test-compile>
                                org.apache.maven.plugins:maven-compiler-plugin:testCompile
                            </test-compile>
                            <test>
                                org.apache.maven.plugins:maven-surefire-plugin:test
                            </test>
                            <package>
                                org.mimacom.maven.plugins:mimacom-liferay-builder-maven-plugin:packageServiceApi,
                                org.apache.maven.plugins:maven-war-plugin:war
                            </package>
                            <install>
                                org.mimacom.maven.plugins:mimacom-liferay-builder-maven-plugin:installService,
                                org.mimacom.maven.plugins:mimacom-liferay-builder-maven-plugin:installWar,
                                org.apache.maven.plugins:maven-install-plugin:install
                            </install>
                            <deploy>
                                org.apache.maven.plugins:maven-deploy-plugin:deploy,
                                org.mimacom.maven.plugins:mimacom-liferay-builder-maven-plugin:deployService,
                            </deploy>
                        </phases>
                    </lifecycle>
                </lifecycles>
            </configuration>
        </component>
        <component>
            <role>org.apache.maven.artifact.handler.ArtifactHandler</role>
            <role-hint>liferayService</role-hint>
            <implementation>
                org.apache.maven.artifact.handler.DefaultArtifactHandler
            </implementation>
            <configuration>
                <type>liferayService</type>
                <extension>jar</extension>
                <language>java</language>
                <addedToClasspath>true</addedToClasspath>
            </configuration>
        </component>
    </components>

</component-set>
